{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": "./",
    "paths": {
      "@src/*": ["src/*"],
      "@features/*": ["src/features/*"],
      "@assets/*": ["assets/*"]
    },
    "types": ["core-js"],
    "typeRoots": ["./types", "./node_modules/@types"],
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowJs": true
  }
}
